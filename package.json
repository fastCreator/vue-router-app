{
  "name": "vue-router-webapp",
  "version": "0.0.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && gh-pages -d dist && npm publish"
  },
  "dependencies": {
    "path-to-regexp": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "vant": "^2.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "description": "vue router for App",
  "main": "lib/index.js",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastCreator/vue-router-webapp.git"
  },
  "keywords": [
    "vue",
    "vue router",
    "app"
  ],
  "author": "coco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fastCreator/vue-router-webapp/issues"
  },
  "homepage": "https://fastcreator.github.io/vue-router-webapp/"
}